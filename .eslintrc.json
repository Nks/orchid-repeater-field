{
    "parser": "@babel/eslint-parser",
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true,
        "jquery": true
    },
    "rules": {
        "indent": [
            "error",
            4
        ],
        "linebreak-style": [
            "error",
            "unix"
        ],
        "quotes": [
            "error",
            "single"
        ],
        "class-methods-use-this": 0,
        "radix": ["error", "as-needed"],
        "no-param-reassign": [2, { "props": false }],
        "import/no-unresolved": "off",
        "import/extensions": "off"
    },
    "extends": "airbnb-base",
    "globals": {
        "axios": true,
        "Turbolinks": true
    }
}
